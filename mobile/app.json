{
  "expo": {
    "name": "iRecipes",
    "slug": "iRecipes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.gabrielpedrotti.iRecipes"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-font",
      [
        "sentry-expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "irecipes"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
